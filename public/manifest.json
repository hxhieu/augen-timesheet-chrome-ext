{
  "name": "Augen Timesheet",
  "version": "1.0",
  "description": "Augen Timesheet extension",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "declarativeContent",
    "https://intranet.augensoftwaregroup.com/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "manifest_version": 2
}