{
  "name": "Augen Timesheet",
  "version": "1.0",
  "description": "Augen Timesheet extension",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "permissions": [
    "cookies",
    "declarativeContent",
    "https://intranet.augensoftwaregroup.com/",
    "http://localhost:5000/"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_icon": "augen-icon.png"
  },
  "manifest_version": 2
}